{
  "name": "ApiIntegrationPlugin",
  "version": "1.0.0",
  "description": "A Python plugin for API integration operations including HTTP requests, webhook handling, and API testing. Supports REST APIs, authentication, and response processing.",
  "language": "Python",
  "entryPoint": "api_integration_plugin.py",
  "capabilities": [
    "http_requests",
    "api_testing",
    "webhook_handling",
    "authentication",
    "response_processing",
    "rate_limiting"
  ],
  "dependencies": [
    "pip:requests>=2.31.0",
    "pip:aiohttp>=3.8.0",
    "pip:pydantic>=2.0.0",
    "pip:httpx>=0.24.0",
    "pip:python-jose[cryptography]>=3.3.0"
  ],
  "configuration": {
    "defaultTimeout": 30,
    "maxRetries": 3,
    "retryDelay": 1,
    "followRedirects": true,
    "validateSsl": true,
    "userAgent": "DevFlow-ApiIntegration/1.0",
    "logLevel": "info"
  }
}

